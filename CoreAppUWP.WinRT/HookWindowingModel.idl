namespace CoreAppUWP.WinRT
{
    enum WindowingModel
    {
        None = 0,
        Universal = 1,
        ClassicDesktop = 2,
        ClassicPhone = 3
    };

    [default_interface]
    runtimeclass HookWindowingModel : Windows.Foundation.IClosable
    {
        HookWindowingModel();

        Boolean IsHooked { get; set; };

        static CoreAppUWP.WinRT.WindowingModel WindowingModel { get; set; };

        void StartHook();
        void EndHook();
    }
}
